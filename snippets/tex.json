{
  "begin": {
    "description": "LaTeX Begin environment",
    "prefix": ["begin"],
    "body": [
      "\\begin{$1}",
      "  $0",
      "\\end{$1}"
    ]
  },
  "definition": {
    "description": "Insert definition environment",
    "prefix": ["def"],
    "body": [
      "\\begin{definition}",
      "  $0",
      "\\end{definition}"
    ]
  },
  "theorem": {
    "description": "Insert theorem environment",
    "prefix": ["theorem"],
    "body": [
      "\\begin{theorem}",
      "  $0",
      "\\end{theorem}"
    ]
  },
  "proof": {
    "description": "Insert proof environment",
    "prefix": ["proof"],
    "body": [
      "\\begin{proof}",
      "  $0",
      "\\end{proof}"
    ]
  },
  "equation": {
    "description": "Insert equation environment",
    "prefix": ["eq"],
    "body": [
