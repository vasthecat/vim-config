      "  $0",
      "\\end{equation*}"
    ]
  },
  "align": {
    "prefix": "align",
    "body": [
      "\\begin{align}",
      "  $0",
      "\\end{align}"
    ],
    "description": "align environment"
  },
  "align*": {
    "prefix": "align*",
    "body": [
      "\\begin{align*}",
      "  $0",
      "\\end{align*}"
    ],
    "description": "align* environment"
  },
  "itemize": {
    "description": "Insert itemize environment",
    "prefix": ["item"],
    "body": [
      "\\begin{itemize}",
      "  \\item $0",
      "\\end{itemize}"
    ]
  },
  "Ignore \item": {
    "description": "Insert itemize environment",
    "prefix": ["\\item"],
    "body": "\\item"
  },
  "enumerate": {
    "description": "Insert enumerate environment",
    "prefix": ["enum"],
    "body": [
      "\\begin{enumerate}",
      "  \\item $0",
      "\\end{enumerate}"
    ]
  },
  "subscript": {
    "description": "Insert multicharacter subscript",
    "prefix": [" _"],
    "body": "_{$1}$0"
  },
  "superscript": {
    "description": "Insert multicharacter superscript",
    "prefix": [" ^"],
    "body": "^{$1}$0"
  },
  "Figure": {
    "prefix": "figure",
    "body": [
      "\\begin{figure}[H]",
      "  \\centering",
      "  \\includegraphics[width=$1\\textwidth]{$2}",
      "  \\caption{$3}",
      "  \\label{fig:$4}",
      "\\end{figure}",
      "$0"
    ],
    "description": "Add a figure"
  },
  "Italic Text": {
    "prefix": ["em", "//", "*"],
    "body": "\\emph{$1}$0",
    "description": "Insert emphasis text."
  },
  "Bold Text": {
    "prefix": ["bf", ";;", "**"],
    "body": "\\textbf{$1}$0",
    "description": "Insert bold text."
  },
  "Bold Italic Text": {
    "prefix": ["bfem", "***"],
    "body": "\\textbf{\\emph{$1}}$0",
    "description": "Insert bold emphasised text."
  },
  "set font size": {
    "prefix": "fontsize",
    "body": "${1|\\Huge,\\huge,\\LARGE,\\Large,\\large,\\normalsize,\\small,\\footnotesize,\\scriptsize,\\tiny|}",
    "description": "Select a font size"
  },

